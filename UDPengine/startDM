while true
do

	ps -C UDPengine
	if [ $? -ne 0 ]
	then
		killall UDPengine
		/home/ubuntu/DM/UDPengine 1>/home/ubuntu/DM/DM.log 2>&1 &
	fi

	ps -C websocketd
	if [ $? -ne 0 ]
	then
		killall websocketd
		/usr/bin/websocketd --port 32160 tail -f /home/ubuntu/DM/DM.log 1>/home/ubuntu/DM/WEBSOCK.log 2>&1 &
	fi
	sleep 60
done
