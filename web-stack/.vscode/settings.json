{
  // "javascript.implicitProjectConfig.checkJs": true,
  "stylelint.enable": true,
  "eslint.enable": true,
  "prettier.disableLanguages": [],
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.formatOnSave": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/build/*/**": true
  },
  "cSpell.words": [
    "Ihlmv",
    "XXXS",
    "caro",
    "datetime",
    "linechart",
    "papaparse",
    "unparse"
  ]
}
